<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo_app2"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Autocompletar" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="78dp"
        android:orientation="horizontal" >
        
        
        <TextView
            android:layout_width="50dp"
            android:layout_height="match_parent"  
            android:layout_weight="8.18"
            android:text="DIRECTORIO ALCALDÍA DE ACACÍAS"
            android:textColor="@android:color/white"
            android:textSize="22dp"
            android:textStyle="bold"
            android:id="@+id/tvDirectorio" />
        

        <ImageView
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:layout_weight="7.02"
            android:src="@drawable/inc" />
    </LinearLayout>


    
    
    
     <ImageButton
            android:id="@+id/ibBuscar2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:src="@drawable/abc_ic_search" />

      <LinearLayout
          android:layout_width="fill_parent"
          android:layout_height="match_parent"
          android:layout_gravity="bottom"
          android:orientation="vertical" >

          <ViewFlipper

              android:layout_width="wrap_content"
              android:layout_height="fill_parent"
              android:id="@+id/vfFlipper"
              android:layout_gravity="right|bottom"
              android:layout_marginTop="100sp"
              

              >

              <ImageView
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_gravity="right|bottom"
                  android:src="@drawable/mano" />

              <ImageView

                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:src="@drawable/mano2"
                  android:layout_gravity="right|bottom"

                  />



          </ViewFlipper>


      </LinearLayout>
    <LinearLayout
        android:layout_height="wrap_content"
        android:layout_width="wrap_content">  

        </LinearLayout>




</LinearLayout>

